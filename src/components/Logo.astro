---
import { Image } from 'astro:assets'

interface Props {
  className?: string
}

const { className } = Astro.props
---

<a href='/'>
  <Image
    class={`${className} cursor-pointer`}
    src='/gyetech-logo.webp'
    alt='logo de guayaquiltech'
    width='50'
    height='50'
    loading='eager'
  />
</a>
